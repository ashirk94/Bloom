<div class="main-wrapper">
	<% if (locals.message) { %>
	<h5><%= locals.message %></h5>
	<% } %>

	<h2 class="title">Values</h2>
	<div id="error"></div>
	<div class="btn-group" id="value-group">
		<div class="draggable" draggable="true" id="family">Family</div>
		<div class="draggable" draggable="true" id="ambition">Ambition</div>
		<div class="draggable" draggable="true" id="compassion">Compassion</div>
		<div class="draggable" draggable="true" id="honesty">Honesty</div>
		<div class="draggable" draggable="true" id="spirituality">
			Spirituality
		</div>
	</div>
	<div class="btn-group">
		<div class="dropzone" id="value1-container"></div>
		<div class="dropzone" id="value2-container"></div>
		<div class="dropzone" id="value3-container"></div>
		<div class="dropzone" id="value4-container"></div>
		<div class="dropzone" id="value5-container"></div>
	</div>
	<div class="btn-group" id="info-group">
		<div class="info">#1</div>
		<div class="info">#2</div>
		<div class="info">#3</div>
		<div class="info">#4</div>
		<div class="info">#5</div>
	</div>
	<form method="post" id="form">
		<input type="hidden" name="value1" id="value-submit1" />
		<input type="hidden" name="value2" id="value-submit2" />
		<input type="hidden" name="value3" id="value-submit3" />
		<input type="hidden" name="value4" id="value-submit4" />
		<input type="hidden" name="value5" id="value-submit5" />
		<div class="btn-group">
			<input class="btn btn-primary" type="submit" value="Submit" />
			<button class="btn" id="reset">Reset</button>
		</div>
	</form>
</div>

<script
	src="https://cdnjs.cloudflare.com/ajax/libs/dragula/3.7.3/dragula.min.js"
	defer
></script>
<script src="/values.js" defer></script>
